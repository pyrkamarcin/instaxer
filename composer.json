{
  "name": "pyrkamarcin/instaxer",
  "description": "Small script to test Instagram PHP SDK libray.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Marcin Pyrka",
      "email": "pyrka.marcin@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Instaxer\\": "src/Instaxer",
      "Monotype\\": "src/Monotype"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "require": {
    "doctrine/doctrine-bundle": "1.*",
    "doctrine/doctrine-cache-bundle": "1.*",
    "doctrine/orm": "2.*",
    "incenteev/composer-parameter-handler": "2.*",
    "sensio/distribution-bundle": "5.*",
    "sensio/framework-extra-bundle": "3.*",
    "symfony/assetic-bundle": "2.*",
    "symfony/monolog-bundle": "3.*",
    "symfony/swiftmailer-bundle": "2.*",
    "symfony/symfony": "3.*",
    "symfony/security-acl": "3.*",
    "react/socket-client": "0.5.*",
    "react/http": "0.4.*",
    "react/stream": "0.4.*",
    "react/datagram": "1.*",
    "ramsey/uuid": "3.*",
    "ramsey/uuid-doctrine": "1.*",
    "ramsey/uuid-console": "1.*",
    "mockery/mockery": "^0.9",
    "phpspec/php-diff": "1.1.*",
    "liamcottle/instagram-sdk-php": "^1.0",
    "phpmailer/phpmailer": "^5.2",
    "phpunit/phpunit": "5.*"
  },
  "require-dev": {
    "sensio/generator-bundle": "3.*",
    "symfony/phpunit-bridge": "3.*"
  },
  "scripts": {
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ]
  },
  "config": {
    "platform": {
      "php": "7.0"
    }
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}
